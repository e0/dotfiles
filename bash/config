export CLICOLOR=1
export LSCOLORS=ExFxCxDxBxegedabagacad

# PS1="\n\u:\w \[$EBLACK\]\[$YELLOW\]\$(parse_git_branch)\[$NO_COLOR\] \nâ†’ "
PS1="\n\w \[$EBLACK\]\[$YELLOW\]\$(parse_git_branch)\[$NO_COLOR\] \n> "

function parse_git_branch {
  git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/(\1)/'
}

xmodmap ~/.Xmodmap

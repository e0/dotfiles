{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "mjs",
    "cjs",
    "typescript",
    "css",
    "less",
    "scss",
    "json",
    "graphql",
    "markdown",
    "vue",
    "yaml",
    "html",
    "svelte",
    "astro"
  ],
  "languageserver": {
		"terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
		  "settings": {}
		}
	},
  "svelte.enable-ts-plugin": true
}
